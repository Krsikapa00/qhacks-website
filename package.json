{
  "name": "qhacks-website",
  "version": "0.0.1",
  "description": "Website for QHacks!",
  "author": "QHacks Engineering",
  "homepage": "https://github.com/qhacks/qhacks-website#readme",
  "bugs": {
    "url": "https://github.com/qhacks/qhacks-website/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bellazhong1029/qhacks-website/tree/qhacks-2021"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "style": "eslint src/**/*.js",
    "clean": "gatsby clean",
    "format": "prettier --write 'src/**/*.js' && eslint src/**/*.js --fix",
    "test": "npm run jest-tests && npm run cypress-tests",
    "jest-tests": "jest",
    "jest-tests-watch": "jest --watchAll",
    "jest-tests-with-coverage": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "cypress-tests": "start-server-and-test develop http://localhost:8000 cypress-run",
    "cypress-run": "cypress run",
    "cypress-open": "cypress open",
    "cypress-run-record": "cypress run --record",
    "cypress-verify": "cypress verify"
  },
  "engines": {
    "node": "12.6.x",
    "npm": "6.x"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "gatsby": "^2.24.11",
    "gatsby-plugin-glamor": "^2.3.10",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-manifest": "^2.4.21",
    "gatsby-plugin-netlify": "^2.3.11",
    "gatsby-plugin-nprogress": "^2.3.10",
    "gatsby-plugin-offline": "^2.0.9",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "glamor": "^2.20.40",
    "lottie-web": "^5.7.1",
    "moment-timezone": "^0.5.31",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-helmet": "^5.2.0",
    "react-responsive": "^5.0.0",
    "react-scrollspy": "^3.3.5",
    "react-slick": "^0.23.1",
    "react-transition-group": "^2.5.0",
    "react-use": "^15.3.3",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "coveralls": "^3.1.0",
    "cypress": "^3.8.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.6.0",
    "regenerator-runtime": "^0.12.1",
    "start-server-and-test": "^1.7.11"
  }
}
